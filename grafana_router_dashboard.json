{
  "id": null,
  "uid": "244e19c6fe23",
  "title": "Router Service \u2013 SLO Dashboard",
  "tags": [
    "router",
    "fastapi",
    "prometheus"
  ],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "5s",
  "templating": {
    "list": [
      {
        "type": "datasource",
        "name": "DS_PROM",
        "query": "prometheus",
        "current": {
          "selected": true
        },
        "label": "Prometheus"
      },
      {
        "type": "query",
        "name": "job",
        "datasource": {
          "type": "prometheus",
          "uid": "${DS_PROM}"
        },
        "query": "label_values(up, job)",
        "current": {
          "text": "router",
          "value": "router"
        },
        "label": "job"
      }
    ]
  },
  "panels": [
    {
      "type": "row",
      "title": "Overview",
      "gridPos": {
        "x": 0,
        "y": 0,
        "w": 24,
        "h": 1
      },
      "collapsed": false,
      "panels": []
    },
    {
      "id": 1,
      "type": "stat",
      "title": "Requests per Second (RPS)",
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROM}"
      },
      "gridPos": {
        "x": 0,
        "y": 1,
        "w": 6,
        "h": 6
      },
      "targets": [
        {
          "expr": "sum(rate(http_requests_total{job=~\"$job\"}[1m]))",
          "refId": "A"
        },
        {
          "expr": "sum(rate(http_server_requests_total{job=~\"$job\"}[1m]))",
          "refId": "B"
        },
        {
          "expr": "sum(rate(fastapi_requests_total{job=~\"$job\"}[1m]))",
          "refId": "C"
        }
      ],
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ]
        },
        "orientation": "auto"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "req/s"
        }
      }
    },
    {
      "id": 2,
      "type": "stat",
      "title": "Error Rate (5xx %)",
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROM}"
      },
      "gridPos": {
        "x": 6,
        "y": 1,
        "w": 6,
        "h": 6
      },
      "targets": [
        {
          "expr": "100 * sum(rate(http_requests_total{job=~\"$job\",status=~\"5..\"}[5m])) / sum(rate(http_requests_total{job=~\"$job\"}[5m]))",
          "refId": "A"
        },
        {
          "expr": "100 * sum(rate(http_server_requests_total{job=~\"$job\",status=~\"5..\"}[5m])) / sum(rate(http_server_requests_total{job=~\"$job\"}[5m]))",
          "refId": "B"
        },
        {
          "expr": "100 * sum(rate(fastapi_requests_total{job=~\"$job\",status=~\"5..\"}[5m])) / sum(rate(fastapi_requests_total{job=~\"$job\"}[5m]))",
          "refId": "C"
        }
      ],
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ]
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green"
              },
              {
                "color": "orange",
                "value": 1
              },
              {
                "color": "red",
                "value": 5
              }
            ]
          }
        }
      }
    },
    {
      "id": 3,
      "type": "stat",
